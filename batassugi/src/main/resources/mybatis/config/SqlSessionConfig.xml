<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <typeAliases>
    <package name="org.spider.batassugi.model" />
  </typeAliases>
  <mappers>
    <mapper resource="mybatis/config/member.xml"/> <!-- 고객 정보에 연결되는 xml --> 
    <mapper resource="mybatis/config/crops.xml"/> <!-- 작물과 연결되는 xml -->
    <mapper resource="mybatis/config/farm.xml"/> <!-- 농지와 관련된 xml -->
    <mapper resource="mybatis/config/recruit.xml"/> <!-- 모집과 관련된 xml -->
    <mapper resource="mybatis/config/admin.xml"/><!-- 관리자 정보에 연결되는 xml -->
    <mapper resource="mybatis/config/sellerFarm.xml"/> <!-- 판매자의 농지관리에 연결되는 xml -->
    <mapper resource="mybatis/config/trade_post.xml"/> <!-- 거래 게시판에 연결되는 xml --> 
    <mapper resource="mybatis/config/rent.xml"/> <!-- 대여에 연결되는 xml -->
    <mapper resource="mybatis/config/buyerFarm.xml"/> <!-- 구매자의 농지관리에 연결되는 xml -->
    <mapper resource="mybatis/config/trade_post_reply.xml"/> <!-- 거래 게시판 댓글 연결되는 xml -->
    <mapper resource="mybatis/config/schedule.xml"/> <!-- 일정 xml -->
 </mappers>
</configuration>










